cat <<EOF >/storage/.config/profile.d/010-governors
CPU_FREQ=("/sys/devices/system/cpu/cpufreq/policy0" "/sys/devices/system/cpu/cpufreq/policy4")
GPU_FREQ=("/sys/devices/platform/soc@0/5000000.gpu/devfreq/5000000.gpu")
EOF
